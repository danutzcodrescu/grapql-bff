{
	"extends": [
		"eslint:recommended",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 8,
		"sourceType": "module"
	},
	"rules": {
		"prettier/prettier": "warn",
		"jest/no-focused-tests": 2,
		"jest/no-identical-title": 2
	},
	"env": {
		"es6": true,
		"node": true,
		"jest": true
	},
	"plugins": [
		"prettier",
		"jest",
		"import"
	]
}
